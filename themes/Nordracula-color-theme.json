	"tokenColors": [
		{
			"scope": [
				"emphasis"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"strong"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"header"
			],
			"settings": {
				"foreground": "#5E81AC"
			}
		},
		{
			"scope": [
				"meta.diff",
				"meta.diff.header"
			],
			"settings": {
				"foreground": "#4C566A"
			}
		},
		{
			"scope": [
				"markup.inserted"
			],
			"settings": {
				"foreground": "#A3BE8C"
			}
		},
		{
			"scope": [
				"markup.deleted"
			],
			"settings": {
				"foreground": "#BF616A"
			}
		},
		{
			"scope": [
				"markup.changed"
			],
			"settings": {
				"foreground": "#D08770"
			}
		},
		{
			"scope": [
				"invalid"
			],
			"settings": {
				"foreground": "#BF616A",
				"fontStyle": "underline italic"
			}
		},
		{
			"scope": [
				"invalid.deprecated"
			],
			"settings": {
				"foreground": "#E5E9F0",
				"fontStyle": "underline italic"
			}
		},
		{
			"scope": [
				"entity.name.filename"
