			"settings": {
				"foreground": "#EBCB8B"
			}
		},
		{
			"scope": [
				"markup.error"
			],
			"settings": {
				"foreground": "#BF616A"
			}
		},
		{
			"name": "Underlined markup",
			"scope": [
				"markup.underline"
			],
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Bold markup",
			"scope": [
				"markup.bold"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#D08770"
			}
		},
		{
			"name": "Markup headings",
